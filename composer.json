{
    "name" : "apnet/assetic-importer-bundle",
    "description" : "Integrates resource projects into Symfony2",
    "keywords" : ["compass", "sass", "css", "scss", "assetic"],
    "type" : "symfony-bundle",
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Andrey F. Mindubaev",
            "email" : "covex.mobile@gmail.com"
        }
    ],
    "require" : {
        "php" : ">=5.3.0",
        "doctrine/lexer" : "~1.0",
        "symfony/assetic-bundle" : "~2.3",
        "symfony/twig-bundle" : "~2.3",
        "symfony/config" : "~2.3",
        "symfony/dependency-injection" : "~2.3",
        "symfony/filesystem" : "~2.3",
        "symfony/finder" : "~2.3",
        "symfony/http-kernel" : "~2.3",
        "symfony/process" : "~2.3",
        "symfony/yaml" : "~2.3",
        "twig/twig" : "~1.12"
    },
    "require-dev" : {
        "symfony/symfony" : "~2.3",
        "symfony/monolog-bundle" : "~2.3",
        "apnet/functional-test-bundle" : "~1.1,>=1.1.4",
        "phpunit/phpunit" : "~4.0",
        "squizlabs/php_codesniffer" : "~1.5",
        "apnet/coding-standard" : "~1.5",
        "covex-nn/twig-callable-bridge-bundle" : "~1.0",
        "knplabs/knp-markdown-bundle" : "~1.3"
    },
    "suggest" : {
        "symfony/event-dispatcher" : "~2.3"
    },
    "autoload" : {
        "psr-0" : {
            "Apnet\\" : "src/"
        }
    },
    "config" : {
        "bin-dir" : "bin"
    },
    "extra" : {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
